@using TatilSeyahatSitesiMVc.Models.Sinflar
@model List<Blog>

<div class="padding1">
    <div class="box-left">
        <h4>İlk Bloglar</h4>
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.Blokimg" alt="" />
            </div>
            <div class="bot-desc">
                <a href="~/Blog/BlogDetay/@x.id"><p>@x.Aciklama.Substring(0, 70) ...</p></a>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5", "AnaSayfa")
</div>
